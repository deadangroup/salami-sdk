{
  "name": "deadangroup/salami-sdk",
  "description": "A PHP SDK to consume Salami",
  "keywords": [
    "support",
    "deadan",
    "hive"
  ],
  "license": "MIT",
  "type": "package",
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "~6|~7",
    "spatie/laravel-webhook-client": ">=2.3.1",
    "illuminate/support": "5.8.*|^6.0|^7.0|^8.0|^9.0",
    "laravel/helpers": "^1.2",
    "psr/log": "^1.1@dev"
  },
  "autoload": {
    "psr-4": {
      "Deadan\\Salami\\": "src"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "authors": [
    {
      "name": "James Ngugi",
      "email": "james@dgl.co.ke"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Deadan\\Salami\\Providers\\ModuleServiceProvider"
      ]
    },
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  }
}
